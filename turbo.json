{
	"$schema": "https://turborepo.org/schema.json",
	"globalDependencies": ["**/.env", "tsconfig.json"],
	"pipeline": {
		"db:generate": {
			"inputs": ["prisma/schema.prisma"],
			"cache": false
		},
		"db:push": {
			"inputs": ["prisma/schema.prisma"],
			"cache": false
		},
		"db:seed": {
			"cache": false
		},
		"db:deploy": {
			"cache": false,
			"inputs": ["prisma/migrations/**/*.sql"]
		},
		"db:migrate": {
			"cache": false,
			"inputs": ["prisma/migrations/**/*.sql"]
		},
		"dev": {
			"persistent": true,
			"cache": false
		},
		"build": {
			"dependsOn": ["^build", "^db:generate"],
			"outputs": [".next/**", ".expo/**"]
		},
		"lint": {},
		"lint:fix": {},
		"clean": {
			"cache": false
		},
		"//#clean": {
			"cache": false
		},
		"type-check": {
			"dependsOn": ["^db:generate"],
			"cache": false
		}
	},
	"globalEnv": [
		"CI",
		"CLERK_SECRET_KEY",
		"CLOUDINARY_URL",
		"CRON_SECRET",
		"DATABASE_URL",
		"EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY",
		"EXPO_ROUTER_APP_ROOT",
		"NEXT_PUBLIC_CLERK_AFTER_SIGN_UP_URL",
		"NEXT_PUBLIC_CLERK_SIGN_UP_URL",
		"NODE_ENV",
		"PORT",
		"SKIP_ENV_VALIDATION",
		"VERCEL_ENV",
		"VERCEL_URL",
		"VERCEL_WEB_ANALYTICS_ID",
		"VERCEL"
	]
}
