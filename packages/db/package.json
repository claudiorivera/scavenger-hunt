{
	"name": "@claudiorivera/db",
	"version": "0.1.0",
	"main": "./index.ts",
	"types": "./index.ts",
	"license": "MIT",
	"scripts": {
		"clean": "rm -rf .turbo node_modules",
		"db:generate": "pnpm with-env prisma generate",
		"db:deploy": "pnpm with-env prisma migrate deploy",
		"db:migrate": "pnpm with-env prisma migrate dev",
		"db:push": "pnpm with-env prisma db push --skip-generate",
		"db:seed": "pnpm with-env prisma db seed",
		"dev": "pnpm with-env prisma studio --port 5556",
		"with-env": "dotenv -e ../../.env --"
	},
	"dependencies": {
		"@claudiorivera/shared": "^0.1.0",
		"@faker-js/faker": "^8.0.2",
		"@prisma/client": "^5.1.0"
	},
	"devDependencies": {
		"dotenv-cli": "^7.2.1",
		"prisma": "^5.1.0",
		"ts-node": "^10.9.1",
		"typescript": "^5.1.6"
	},
	"prisma": {
		"seed": "ts-node --transpile-only ./run-seed.ts"
	}
}
