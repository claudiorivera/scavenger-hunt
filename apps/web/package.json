{
	"name": "@claudiorivera/web",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"build": "pnpm with-env next build",
		"clean": "git clean -xdf .next .turbo node_modules",
		"dev": "pnpm with-env next dev --turbopack",
		"start": "pnpm with-env next start",
		"check": "pnpm biome check src",
		"check:fix": "pnpm biome check --write src",
		"check-types": "tsc --noEmit",
		"with-env": "dotenv -e ../../.env --"
	},
	"dependencies": {
		"@claudiorivera/api": "workspace:*",
		"@claudiorivera/auth": "workspace:*",
		"@claudiorivera/db": "workspace:*",
		"@claudiorivera/shared": "workspace:*",
		"@hookform/resolvers": "3.9.1",
		"@t3-oss/env-nextjs": "0.11.1",
		"@tanstack/react-query": "4.36.1",
		"@tanstack/react-query-next-experimental": "5.62.2",
		"@trpc/client": "10.45.2",
		"@trpc/next": "10.45.2",
		"@trpc/react-query": "10.45.2",
		"@trpc/server": "10.45.2",
		"@vercel/analytics": "1.4.1",
		"axios": "1.7.9",
		"classnames": "2.5.1",
		"daisyui": "4.12.14",
		"next": "15.0.4",
		"react": "19.0.0",
		"react-dom": "19.0.0",
		"react-hook-form": "7.53.2",
		"react-hot-toast": "2.4.1",
		"react-icons": "5.4.0",
		"sharp": "0.33.5",
		"superjson": "2.2.2",
		"zod": "3.23.8"
	},
	"devDependencies": {
		"@claudiorivera/tailwind-config": "workspace:*",
		"@claudiorivera/tsconfig": "workspace:*",
		"@types/node": "22.10.1",
		"@types/react": "npm:types-react@19.0.0-rc.1",
		"@types/react-dom": "npm:types-react-dom@19.0.0",
		"dotenv-cli": "7.4.4",
		"tailwindcss": "3.4.16",
		"typescript": "5.7.2"
	},
	"pnpm": {
		"overrides": {
			"@types/react": "npm:types-react@19.0.0-rc.1",
			"@types/react-dom": "npm:types-react-dom@19.0.0"
		}
	}
}
